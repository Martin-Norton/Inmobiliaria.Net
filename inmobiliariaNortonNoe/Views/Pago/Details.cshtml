@model inmobiliariaNortonNoe.Models.Inquilino

@{
    ViewData["Title"] = "Detalles del Inquilino";
}

<h2>Detalles del Inquilino</h2>

<div class="form-group">
    <label>Nombre</label>
    <p>@Model.Nombre</p>
</div>

<div class="form-group">
    <label>Apellido</label>
    <p>@Model.Apellido</p>
</div>

<div class="form-group">
    <label>DNI</label>
    <p>@Model.Dni</p>
</div>

<div class="form-group">
    <label>Email</label>
    <p>@Model.Email</p>
</div>

<div class="form-group">
    <label>Tel√©fono</label>
    <p>@Model.Telefono</p>
</div>

<a asp-action="Index" class="btn btn-secondary">Volver</a>
<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a>
<a asp-action="Eliminar" asp-route-id="@Model.Id" class="btn btn-danger">Eliminar</a>
